<ul id='requests'>


  <li>
    <% @relevant_bookings.each do |booking| %>
      <% booking.each do |b| %>
        <% l = Listing.all(id: b.listing_id) %>
        Title = <%= l[0].title %>
        start_date: <%= b.start_date %>
        end_date: <%= b.end_date %>
        confirmation: <%= b.confirmation %>
      <%end%>
    <% end %>
  </li>
</ul>
