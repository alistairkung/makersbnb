<h1>Listings</h1>
<ul id='listings'>
  <% @listings.each do |listing| %>
  <li>
        Title: <%= listing.title %>
        Location: <%= listing.location %>
        Beds: <%= listing.beds %>
        Price: <%= listing.price %>
        Max guests: <%= listing.max_guests %>
      </li>
    <% end %>
</ul>
